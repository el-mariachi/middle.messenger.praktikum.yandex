{
  "name": "messenger",
  "version": "1.0.0",
  "description": "Yandex Praktikum Middle Frontend Developer Course Project",
  "scripts": {
    "test": "mocha",
    "dev": "parcel static/index.handlebars",
    "clean:dist": "rimraf dist",
    "build": "npm run clean:dist && parcel build static/index.handlebars && tsc",
    "start": "node dist/server",
    "start:local": "ts-node server.ts",
    "lint": "eslint \"src/**/*.ts\" --ignore-path .gitignore",
    "lint:style": "stylelint \"src/**/*.{scss,css}\" --ignore-path .gitignore",
    "format": "prettier \"**/*.{ts,hbs,json}\" --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/el-mariachi/middle.messenger.praktikum.yandex.git"
  },
  "keywords": [
    "messenger",
    "yandex"
  ],
  "author": "Dmitri Sokolov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/el-mariachi/middle.messenger.praktikum.yandex/issues"
  },
  "homepage": "https://github.com/el-mariachi/middle.messenger.praktikum.yandex#readme",
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "@types/express": "4.17.14",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "handlebars": "4.7.7",
    "parcel": "2.7.0",
    "parcel-transformer-handlebars": "1.0.0",
    "parcel-transformer-hbs": "1.0.4",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "stylelint": "^14.14.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "dependencies": {
    "express": "4.18.2"
  }
}
